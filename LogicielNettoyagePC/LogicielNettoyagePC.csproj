<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationManifest>app.manifest</ApplicationManifest>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ico-list.png" />
    <None Remove="ico.ico" />
    <None Remove="icons8-balayer-100.png" />
    <None Remove="icons8-domaine-64.png" />
    <None Remove="icons8-mettre-à-jour-la-rotation-à-gauche-100.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="ico-list.png" />
    <Resource Include="ico.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="icons8-balayer-100.png" />
    <Resource Include="icons8-domaine-64.png" />
    <Resource Include="icons8-mettre-à-jour-la-rotation-à-gauche-100.png" />
  </ItemGroup>

</Project>
